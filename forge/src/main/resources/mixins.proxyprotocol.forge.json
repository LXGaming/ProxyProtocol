  "minVersion": "0.8",
  "package": "io.github.lxgaming.proxyprotocol.forge.mixin.forge",
  "refmap": "mixins.proxyprotocol.refmap.json",
  "target": "@env(DEFAULT)",
  "compatibilityLevel": "JAVA_8",
